package main;


func fun(a, b int) ( int,  int,  int) {
    z := 99;
    if a > b {
        return a+b, b, z;
    }
    else {
        return b-a, b, z;
    };
};

func fun2(a, b int) int {
    return a;
};


func main() {
    var a int = 2;
    b := 3;
    var c, d int;
    c, d, b = fun(a, b);
    print %d c;
    print %d d;
    print %d b;

    c = fun2(a, b);
    print %d c;
};

