package main;

func fib(n int) int {
    if n == 0 {
        return 1;
    };
    if n == 1 {
        return 1;
    };
    return fib(n-1) + fib(n-2);
};

func fact(n int) int {
    if n == 1 {
        return 1;
    };
    return n * fact(n-1);
};
 

func main() {
    m := 6;
    n := 2;
    var x int;

    if m > n {
        // print %d m;
        x = fib(m);
    }
    else {
        // print %d n;
        x = fact(n);
    };

    print %d x;
};