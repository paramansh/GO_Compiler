package main;

func sort(a []int, n int) {
    var i, j int;
    for i = 0; i < n-1; i++ {
       for j = 0; j < n-i-1; j++ {
           if (a[j] > a[j+1]) {
                temp := a[j];
                a[j] = a[j+1];
                a[j+1] = temp;
           };
       };
    };   
    return;
};

